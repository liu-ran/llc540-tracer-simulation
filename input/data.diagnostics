#                'DFrE_SLT',
#frequency(24) = 315360000.,
#filename(24) = 'salt_state',

# ETAN
# fields(1,4) = 'ETAN',
# frequency(4) = -31536000.,
# filename(4) = 'ETAN',
 
# 3D state variables
 fields(1:3,3) = 'UVEL ', 'VVEL ', 'WVEL ',
 frequency(3) = 31536000.,
 filename(3) = 'state_3d1',
 
 fields(1:4,2) = 'RHOAnoma ','SALT','THETA','DRHODR',
