#fields(1:5,31) = 'oceTAUX','oceTAUY','oceFWflx','oceQnet',
#'ETAN',
#frequency(31) = 315360000.,
#filename(31) = 'state_2d',
 
 fields(1:2,1) = 'momKE   ','momVort3',
